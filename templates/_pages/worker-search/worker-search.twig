{% extends '_layouts/layout_base' %}

{% block body %}

    {% include '_pages/worker-search/worker-search-form' %}

    {% set workers = craft.entries()
        .section('workerSingle')
        .with(['photo'])
        .orderBy('title DESC')
        .collect()
    %}

    {% include '_components/worker-list-section' with {
        workers: workers,
        showMore: false,
    } only %}

{% endblock %}